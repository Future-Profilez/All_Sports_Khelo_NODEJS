  @@index([city_id], map: "academy_city_id")
  @@index([name], map: "academy_name")
  @@index([slug_name], map: "academy_slug_name")
  @@index([state_id, city_id], map: "academy_state_city_id")
  @@index([state_id], map: "academy_state_id")
