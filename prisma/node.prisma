model ask_notes {
  id        Int      @id @default(autoincrement())
  title     String   @db.VarChar(255)
  body      String?  @db.LongText
  createdAt DateTime @default(now()) @db.Timestamp(0)
}

model tournaments {
  id                            Int       @id
  uuid                          String    @db.Char(36)
  sport_id                      String?   @db.Char(36)
  created_by                    BigInt?   @db.UnsignedBigInt
  name                          String?   @db.VarChar(255)
  slug_name                     String?   @db.VarChar(255)
  locality                      String?   @default("local") @db.VarChar(255)
  events_payload                String?   @db.LongText
  match_type                    String?   @db.VarChar(255)
  description                   String?   @db.LongText
  type                          String?   @default("real") @db.VarChar(255)
  tournament_type               String?   @db.VarChar(255)
  user_id                       BigInt?   @db.UnsignedBigInt
  academy_id                    Int?
  tourstartdate                 DateTime? @db.Date
  tourenddate                   DateTime? @db.Date
  sets                          Int?
  tournament_set                String?   @db.VarChar(255)
  tourcategory                  String?   @db.VarChar(255)
  drawtype                      String?   @db.VarChar(255)
  agegroup                      String?   @db.VarChar(255)
  variationid                   String?   @db.VarChar(255)
  venue                         String?   @db.VarChar(255)
  official                      String?   @db.VarChar(255)
  tourimage                     String?   @db.VarChar(255)
  winnerprize                   String?   @db.VarChar(255)
  contestantprize               String?   @db.VarChar(255)
  semifinalprize                String?   @db.VarChar(255)
  quarterfinalprize             String?   @db.VarChar(255)
  runnerupprize                 String?   @db.VarChar(255)
  long                          String?   @db.VarChar(255)
  lat                           String?   @db.VarChar(255)
  country_id                    BigInt?   @db.UnsignedBigInt
  state_id                      BigInt?   @db.UnsignedBigInt
  city_id                       BigInt?   @db.UnsignedBigInt
  address                       String?   @db.Text
  winner_id                     Int?
  organization_tier_id          String?   @db.Char(36)
  status                        Boolean   @default(false)
  participation_limit           Int       @default(1) @db.TinyInt
  publish_status                Int       @default(0) @db.TinyInt
  is_old                        Int       @default(1)
  is_verified                   Int       @default(1)
  created_at                    DateTime? @db.Timestamp(0)
  updated_at                    DateTime? @db.Timestamp(0)
  is_whatsapp_message_allowed   Boolean   @default(false)
  tournament_fees               Decimal?  @db.Decimal(10, 2)
  is_event_fees                 Boolean   @default(false)
  is_registration_table_dynamic Boolean   @default(false)
  is_razorpay_payment_enable    Boolean   @default(false)
  deleted_at                    DateTime? @db.Timestamp(0)

  contents tournament_contents[]
}
