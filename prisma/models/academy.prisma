model academies {
  id                 BigInt    @id @db.UnsignedBigInt
  uuid               String    @db.Char(36)
  user_id            BigInt?   @db.UnsignedBigInt
  name               String    @db.VarChar(255)
  slug_name          String?   @db.VarChar(255)
  owner_name         String?   @db.VarChar(255)
  owner_phone_prefix String?   @default("91") @db.VarChar(255)
  owner_phone        String?   @db.VarChar(255)
  owner_email        String?   @db.VarChar(255)
  website            String?   @db.VarChar(255)
  address            String    @db.VarChar(255)
  pincode            Int?
  lat                String?   @db.VarChar(255)
  long               String?   @db.VarChar(255)
  country_id         BigInt?   @default(105) @db.UnsignedBigInt
  state_id           String?   @db.VarChar(255)
  city_id            String?   @db.VarChar(255)
  status             Int       @default(0) @db.TinyInt
  booking            Int       @default(0) @db.TinyInt
  is_active          Int       @default(1)
  is_verified        Int       @default(0)
  created_at         DateTime? @db.Timestamp(0)
  updated_at         DateTime? @db.Timestamp(0)
  deleted_at         DateTime? @db.Timestamp(0)
  morning            BigInt?
  evening            BigInt?

  academy_seo_contents academy_seo_contents[] @relation("AcademySeoRelation")
  academy_amenities academy_amenities[] @relation("AcademyAmentiesRelation")
  academy_details academy_details[] @relation("AcademyDetailsRelation")
  academy_timings academy_timings[] @relation("AcademyTimingRelation")
  academy_programs academy_programs[] @relation("AcademyProgramsRelation")
  academy_coaches academy_coaches[] @relation("AcademyCoachesRelation")
  academy_galleries academy_galleries[] @relation("AcademyGalleryRelation")
  academy_achievements academy_achievements[] @relation("AcademyAchievementsRelation")
}
 
model academy_seo_contents {
  id         BigInt    @id @db.UnsignedBigInt
  academy_id BigInt    @db.UnsignedBigInt
  name       String?   @db.VarChar(255)
  value      String?   @db.LongText
  extra      String?   @db.LongText
  updated_by BigInt?   @db.UnsignedBigInt
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)
  deleted_at DateTime? @db.Timestamp(0)

  academies academies @relation("AcademySeoRelation", fields: [academy_id], references: [id])
}

model academy_amenities {
  id           BigInt    @id @db.UnsignedBigInt
  user_id      String?   @db.VarChar(255)
  academy_id   BigInt?   @db.UnsignedBigInt
  amenity_name String    @db.VarChar(255)
  created_at   DateTime? @db.Timestamp(0)
  updated_at   DateTime? @db.Timestamp(0)
  deleted_at   DateTime? @db.Timestamp(0)
  academies   academies? @relation("AcademyAmentiesRelation", fields: [academy_id], references: [id])
}
model academy_details {
  id              BigInt    @id @db.UnsignedBigInt
  academy_id      BigInt    @db.UnsignedBigInt
  timing          String?   @db.VarChar(255)
  amenities       String?   @db.VarChar(255)
  rooms           String?   @db.VarChar(255)
  img             String?   @db.LongText
  profile         String?   @db.VarChar(255)
  additional_info String?   @db.LongText
  h1_tag          String?   @db.VarChar(255)
  created_at      DateTime? @db.Timestamp(0)
  updated_at      DateTime? @db.Timestamp(0)
  timings_to      String?   @db.VarChar(255)
  timings_from    String?   @db.VarChar(255)
  deleted_at      DateTime? @db.Timestamp(0)
  academies   academies? @relation("AcademyDetailsRelation", fields: [academy_id], references: [id])
}
model academy_timings {
  id            String    @id @db.Char(36)
  academy_id    BigInt    @db.UnsignedBigInt
  morning_shift String?   @db.LongText
  evening_shift String?   @db.LongText
  created_at    DateTime? @db.Timestamp(0)
  updated_at    DateTime? @db.Timestamp(0)
  deleted_at    DateTime? @db.Timestamp(0)

  academies academies @relation("AcademyTimingRelation", fields: [academy_id], references: [id])
  @@index([academy_id], map: "academy_timings_academy_id_foreign")
}
model academy_urls {
  id           BigInt    @id @db.UnsignedBigInt
  uuid         String    @db.Char(36)
  user_id      BigInt    @db.UnsignedBigInt
  insta_url    String?   @db.VarChar(255)
  youtube_url  String?   @db.VarChar(255)
  twitter_url  String?   @db.VarChar(255)
  fb_url       String?   @db.VarChar(255)
  linkedln_url String?   @db.VarChar(255)
  website_url  String?   @db.VarChar(255)
  other_url    String?   @db.VarChar(255)
  created_at   DateTime? @db.Timestamp(0)
  updated_at   DateTime? @db.Timestamp(0)
}
model academy_programs {
  id             BigInt    @id @db.UnsignedBigInt
  default_id     BigInt?   @db.UnsignedBigInt
  academy_id     BigInt?   @db.UnsignedBigInt
  program_name   String    @db.VarChar(255)
  training_time  String    @db.VarChar(255)
  services       String?   @db.VarChar(255)
  other_services String?   @db.VarChar(255)
  created_at     DateTime? @db.Timestamp(0)
  updated_at     DateTime? @db.Timestamp(0)
  deleted_at     DateTime? @db.Timestamp(0)
  academies   academies? @relation("AcademyProgramsRelation", fields: [academy_id], references: [id])
}
model academy_coaches {
  id                 BigInt    @id @db.UnsignedBigInt
  uuid               String    @db.Char(36)
  academy_id         BigInt    @db.UnsignedBigInt
  image              String?   @db.VarChar(255)
  name               String?   @db.VarChar(255)
  email              String?   @db.VarChar(255)
  phone              String?   @db.VarChar(255)
  achievement        String?   @db.Text
  overview           String?   @db.Text
  experience         Int?
  facebook           String?   @db.VarChar(255)
  twitter            String?   @db.VarChar(255)
  instagram          String?   @db.VarChar(255)
  youtube_video_link String?   @db.VarChar(255)
  created_at         DateTime? @db.Timestamp(0)
  updated_at         DateTime? @db.Timestamp(0)
  deleted_at         DateTime? @db.Timestamp(0)
  phone_prefix       String?   @default("91") @db.VarChar(255)
  academies   academies? @relation("AcademyCoachesRelation", fields: [academy_id], references: [id])
}

model academy_galleries {
  id                  BigInt    @id @db.UnsignedBigInt
  uuid                String    @db.Char(36)
  academy_id          BigInt    @db.UnsignedBigInt
  academy_name        String    @db.VarChar(255)
  slug_name           String?   @db.VarChar(255)
  title               String?   @db.VarChar(255)
  academy_description String    @db.LongText
  academy_images      String    @db.LongText
  created_at          DateTime? @db.Timestamp(0)
  updated_at          DateTime? @db.Timestamp(0)
  deleted_at          DateTime? @db.Timestamp(0)
  academies   academies? @relation("AcademyGalleryRelation", fields: [academy_id], references: [id])
}

model academy_achievements {
  id                 BigInt    @id @db.UnsignedBigInt
  uuid               String?   @db.VarChar(255)
  name               String    @db.VarChar(255)
  slug_name          String?   @db.VarChar(255)
  achhievement_image String?   @db.VarChar(255)
  description        String?   @db.LongText
  created_at         DateTime? @db.Timestamp(0)
  updated_at         DateTime? @db.Timestamp(0)
  academy_id         BigInt    @db.UnsignedBigInt
  deleted_at         DateTime? @db.Timestamp(0)
  academies   academies? @relation("AcademyAchievementsRelation", fields: [academy_id], references: [id])
}