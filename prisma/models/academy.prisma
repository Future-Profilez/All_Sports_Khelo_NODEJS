model academies {
  id                 BigInt    @id @db.UnsignedBigInt
  uuid               String    @db.Char(36)
  user_id            BigInt?   @db.UnsignedBigInt
  name               String    @db.VarChar(255)
  slug_name          String?   @db.VarChar(255)
  owner_name         String?   @db.VarChar(255)
  owner_phone_prefix String?   @default("91") @db.VarChar(255)
  owner_phone        String?   @db.VarChar(255)
  owner_email        String?   @db.VarChar(255)
  website            String?   @db.VarChar(255)
  address            String    @db.VarChar(255)
  pincode            Int?
  lat                String?   @db.VarChar(255)
  long               String?   @db.VarChar(255)
  country_id         BigInt?   @default(105) @db.UnsignedBigInt
  state_id           String?   @db.VarChar(255)
  city_id            String?   @db.VarChar(255)
  status             Int       @default(0) @db.TinyInt
  booking            Int       @default(0) @db.TinyInt
  is_active          Int       @default(1)
  is_verified        Int       @default(0)
  created_at         DateTime? @db.Timestamp(0)
  updated_at         DateTime? @db.Timestamp(0)
  deleted_at         DateTime? @db.Timestamp(0)
  morning            BigInt?
  evening            BigInt?

  academy_seo_contents academy_seo_contents[] @relation("AcademySeoRelation")
  academy_amenities academy_amenities[] @relation("AcademyAmentiesRelation")
}

model academy_seo_contents {
  id         BigInt    @id @db.UnsignedBigInt
  academy_id BigInt    @db.UnsignedBigInt
  name       String?   @db.VarChar(255)
  value      String?   @db.LongText
  extra      String?   @db.LongText
  updated_by BigInt?   @db.UnsignedBigInt
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)
  deleted_at DateTime? @db.Timestamp(0)

  academies academies @relation("AcademySeoRelation", fields: [academy_id], references: [id])
}

model academy_amenities {
  id           BigInt    @id @db.UnsignedBigInt
  user_id      String?   @db.VarChar(255)
  academy_id   BigInt?   @db.UnsignedBigInt
  amenity_name String    @db.VarChar(255)
  created_at   DateTime? @db.Timestamp(0)
  updated_at   DateTime? @db.Timestamp(0)
  deleted_at   DateTime? @db.Timestamp(0)
  academies   academies? @relation("AcademyAmentiesRelation", fields: [academy_id], references: [id])
}